<?xml version="1.0" encoding="utf-8"?>
<rotate xmlns:android="http://schemas.android.com/apk/res/android" android:toDegrees="270" android:fromDegrees="270">
<layer-list>
    <item>
            <shape
                android:shape="rectangle"
                android:useLevel="false">
                <solid android:color="@color/Accent_10" />
                <corners android:radius="30dp"/>
            </shape>

    </item>
    <item>
        <clip>
                <shape
                    android:shape="rectangle"
                    android:useLevel="false">
                    <gradient android:centerColor="@color/Accent" android:endColor="@color/Accent" android:startColor="@color/start_accent"/>
                    <corners android:radius="30dp"/>
                </shape>

        </clip>
    </item>

</layer-list>
</rotate>